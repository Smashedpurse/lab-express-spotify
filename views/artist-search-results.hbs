{{#each items}}
    <div class="row">
    {{#each images}}
        {{#if @last}}
    <div class="card" style="width: 18rem;">
    
    <img src="{{this.url}}">
        {{/if}}
        {{/each}}
            <div class="card-body">
            <h5 class="card-title"><p class="text-center">{{name}}</p></h5>
            <a href="/albums/{{id}}" class="btn btn-primary" style="position: relative;left: 67px;">View Albums</a>
            </div>
        </div>
    </div>
    
{{/each}}

